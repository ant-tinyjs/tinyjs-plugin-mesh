{
  "name": "tinyjs-plugin-mesh",
  "version": "0.0.4",
  "author": "fangjun.yfj",
  "description": "The Tiny.js plugin of mesh",
  "keywords": [
    "tinyjs",
    "plugin"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ant-tinyjs/tinyjs-plugin-mesh.git"
  },
  "main": "dist/index.debug.js",
  "files": [
    "dist",
    "src",
    "demo",
    "test"
  ],
  "scripts": {
    "start": "webpack-dev-server --devtool source-map --inline --hot --host 0.0.0.0",
    "prebuild": "npm run ci",
    "build": "webpack --progress --colors",
    "predoc": "rimraf docs",
    "doc": "jsdoc README.md src -r -d docs -t node_modules/docdash",
    "lint": "eslint src test/unit/**/*.test.js",
    "test": "mocha ./test/unit/**/*.test.js --recursive --require ./test/setup.js",
    "ci": "npm run lint && npm run test",
    "preversion": "npm run ci",
    "postversion": "git push --tags",
    "prepublish": "npm run build"
  },
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-loader": "^6.3.1",
    "babel-plugin-static-fs": "^1.1.0",
    "babel-preset-es2015": "^6.22.0",
    "chai": "~3.5.0",
    "docdash": "^0.4.0",
    "eslint": "~3.5.0",
    "eslint-config-standard": "~6.0.1",
    "eslint-plugin-promise": "~3.4.0",
    "eslint-plugin-standard": "~2.0.1",
    "jsdoc": "~3.4.2",
    "jsdom": "^11.3.0",
    "mocha": "^3.4.2",
    "rimraf": "^2.6.1",
    "sinon": "~1.17.6",
    "sinon-chai": "~2.8.0",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-merge": "~0.15.0"
  },
  "bugs": {
    "url": "https://github.com/ant-tinyjs/tinyjs-plugin-mesh/issues"
  },
  "homepage": "https://github.com/ant-tinyjs/tinyjs-plugin-mesh#readme"
}
